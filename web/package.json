{
  "scripts": {
    "dev": "npx parcel src/index.html --open",
    "build": "npx parcel build src/index.html"
  },
  "devDependencies": {
    "parcel": "^2.3.2",
    "parcel-reporter-static-files-copy": "^1.3.4"
  },
  "staticFiles": {
    "staticPath": "public"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  }
}